<h2>Blog Posts</h2>

<ul class="collection">
  <%= for blog_post <- @blog_posts do %>
    <li class="collection-item">
      <%= link blog_post.title, to: blog_path(@conn, :show, blog_post) %> 
    </li>
  <% end %>
</ul>

<%= link to: blog_path(@conn, :new), class: "btn waves-effect waves-light black" do %>
    <i class="material-icons">Add New Blog Post</i>
<% end %>
